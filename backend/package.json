{
  "name": "ocelot-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "LGPL-3.0-only",
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "serve": "node dist/testServer",
    "deploy": "gcloud functions deploy --trigger-http ocelot --runtime nodejs14 --entry-point paws --allow-unauthenticated",
    "deploy-beta": "gcloud functions deploy --trigger-http ocelot-beta --runtime nodejs8 --project arjunguha-research-group --entry-point paws"
  },
  "dependencies": {
    "@google-cloud/datastore": "~6",
    "@google-cloud/error-reporting": "~2",
    "@google-cloud/storage": "~5",
    "body-parser": "^1.18.3",
    "cors": "~2",
    "express": "^4.16.3",
    "google-auth-library": "~6",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "~14",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/uid-safe": "^2.1.1",
    "typescript": "~4"
  }
